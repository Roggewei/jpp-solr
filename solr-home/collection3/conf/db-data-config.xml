<dataConfig>
    <dataSource driver="com.mysql.jdbc.Driver" url="jdbc:mysql://localhost:3306/db_yotaphone" user="root" password="root"/>
    <document>
      <entity name="customer"  query="select id,user_name,mobile,company_name from yt_customer">
       <field name="id" column="id" />
